<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    
    <!-- Plugin Informations -->
    <name>contact</name>
    <class>fr.paris.lutece.plugins.contact.service.ContactPlugin</class>
    <version>3.0.2</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>contact.plugin.description</description>
    <provider>contact.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/contact/contact.png</icon-url>
    <copyright>Copyright 2001-2012 Mairie de Paris</copyright>
    <core-version-dependency>
		<min-core-version>4.0.0</min-core-version>
		<max-core-version/>	
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>
    
    <admin-features>
        <admin-feature>
            <feature-id>CONTACT_MANAGEMENT</feature-id>
            <feature-title>contact.adminFeature.contact_management.name</feature-title>
            <feature-description>contact.adminFeature.contact_management.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/contact/ManageContactsHome.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/contact/contact_feature.png</feature-icon-url>
        </admin-feature>
    </admin-features>
    
    <!-- Applications -->
    <applications>
        <application>
            <application-id>contact</application-id>
            <application-class>fr.paris.lutece.plugins.contact.web.ContactApp</application-class>
        </application>
    </applications>
</plug-in>